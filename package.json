{
    "name": "ping-pong",
    "version": "2.0.4",
    "description": "Ping-Pong PWA (Progressive Web Application)",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.conf.js",
        "build": "cross-env NODE_ENV=production webpack --config config/webpack.build.conf.js",
        "docs": "jsdoc -c jsdoc.json",
        "deploy": "gh-pages -d dist",
        "eslint-fix": "eslint --fix --ext .js ."
    },
    "browserslist": [
        "defaults"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/corocoto/ping-pong.git"
    },
    "keywords": [
        "Ping-pong",
        "JavaScript",
        "Game",
        "PWA",
        "Progressive Web Application"
    ],
    "author": "Techsoftmedia",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.14.2",
        "@babel/preset-env": "^7.14.2",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.4",
        "css-mqpacker": "^7.0.0",
        "cssnano": "^5.0.2",
        "eslint": "^7.26.0",
        "html-webpack-plugin": "^5.3.1",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-webpack-plugin": "^2.4.2",
        "jsdoc": "^3.6.7",
        "mini-css-extract-plugin": "^1.6.0",
        "postcss": "^8.2.15",
        "postcss-loader": "^5.3.0",
        "style-loader": "^2.0.0",
        "webpack": "^5.37.0",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
    },
    "dependencies": {
        "core-js": "^3.12.1",
        "gh-pages": "^3.1.0"
    }
}